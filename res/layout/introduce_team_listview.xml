<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:id="@+id/team_item_image" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_margin="5px" />
	<TextView android:id="@+id/team_item_name" android:layout_width="wrap_content"
		android:layout_height="wrap_content" style="@style/softwarefontcolor"
		android:textSize="13px" />
</LinearLayout>